{
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 2,
        "useTabs": false,
        "endOfLine": "lf",
        "printWidth": 120,
        "singleQuote": true,
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "jsxSingleQuote": true,
        "trailingComma": "all",
        "bracketSameLine": false
      }
    }
  ]
}
